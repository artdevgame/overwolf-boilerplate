{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "lib": ["ES5", "ES2017", "DOM", "ScriptHost"],
    "jsx": "react",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "~/*": ["./*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
